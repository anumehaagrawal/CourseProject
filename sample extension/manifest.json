{
    "manifest_version": 2,
  
    "name": "Sample extension",
    "description": "This extension will analyze a page",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      
    "permissions": [
     "activeTab",
     "tabs",
     "http://127.0.0.1:8000/"
     ]
  }
  